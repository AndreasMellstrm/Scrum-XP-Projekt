@model Örebro_Universitet_Kommunikation.Models.ProjectViewModel
@{
    ViewBag.Title = "Index";
}
<div class="list-group">
    <h3 class="list-group-item list-group-item-action active">Kategorier:</h3>
    @foreach (var p in Model.ProjectItems) {
        @Html.ActionLink(p.ProjectName, "ShowResearch", "ResearchBlog", new { ResearchProject = p.Id }, new { @class = "list-group-item list-group-item-action" })
        }
</div>